{
    "name": "vscode-bisect",
    "version": "0.3.6",
    "description": "Bisect released VS Code Insider builds to find bugs or performance issues similar to what git bisect supports.",
    "repository": {
        "type": "git",
        "url": "https://github.com/bpasero/vscode-bisect"
    },
    "homepage": "https://github.com/bpasero/vscode-bisect",
    "keywords": [
        "vscode"
    ],
    "license": "MIT",
    "authors": "bpasero",
    "main": "out/index",
    "bin": {
        "vscode-bisect": "bin/vscode-bisect"
    },
    "scripts": {
        "compile": "tsc -p ./",
        "build": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "prepare": "npm run build"
    },
    "engines": {
        "node": ">= 16"
    },
    "dependencies": {
        "@vscode/vscode-perf": "^0.0.6",
        "chalk": "^4.x",
        "cli-progress": "^3.11.2",
        "commander": "^9.4.0",
        "open": "^8.4.0",
        "playwright": "^1.29.2",
        "prompts": "^2.4.2",
        "simple-git": "^3.16.0",
        "tree-kill": "^1.2.2",
        "vscode-uri": "^3.0.7"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.0",
        "@types/node": "18.x",
        "@types/prompts": "^2.0.14",
        "typescript": "4.8.x"
    }
}
